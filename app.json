	"flow": {
		"triggers": [{
			"id": "presenceChanged",
			"title": {
				"en": "Presence changed",
				"nl": "Aanwezigheid veranderd"
			},
			"args": [{
				"name": "id",
				"type": "text",
				"placeholder": {
					"en": "Device ID",
					"nl": "Apparaat ID"
				}
			}],
			"tokens": [{
				"name": "isPresent",
				"title": {
					"en": "Present",
					"nl": "Aanwezig"
				}
			}]
		}, {
			"id": "presenceChangedAny",
			"title": {
				"en": "Presence changed",
				"nl": "Aanwezigheid veranderd"
			},
			"tokens": [{
				"name": "isPresent",
				"title": {
					"en": "Present",
					"nl": "Aanwezig"
				}
			}, {
				"name": "id",
				"title": {
					"en": "ID",
					"nl": "ID"
				}
			}]
		}]
	}
